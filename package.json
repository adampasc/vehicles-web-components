{
  "name": "vehicles-ngx-webcomponents",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "serve:host": "ng serve host --configuration=production --single-bundle",
    "serve:a": "ng serve webcomponent-a --configuration=production --single-bundle --live-reload=false",
    "serve:b": "ng serve webcomponent-b --configuration=production --single-bundle --live-reload=false",
    "serve:all": "concurrently --kill-others \"npm run serve:host\" \"npm run serve:a\" \"npm run serve:b\"",
    "postinstall:bak": "ngcc",
    "postinstall": "ngcc",
    "build:a:stats": "ng build --project webcomponent-a --stats-json",
    "analyze:a": "webpack-bundle-analyzer dist/webcomponent-a/stats.json",
    "build:webcomponent-a": "ng build --configuration=production --project webcomponent-a --single-bundle",
    "build:webcomponent-b": "ng build --configuration=production --project webcomponent-b --single-bundle",
    "build:host": "ng build --configuration=production --project host --single-bundle"
  },
  "private": true,
  "dependencies": {
    "@angular-extensions/elements": "^15.3.0",
    "@angular/animations": "^15.2.0",
    "@angular/common": "^15.2.0",
    "@angular/compiler": "^15.2.0",
    "@angular/core": "^15.2.0",
    "@angular/elements": "^15.2.1",
    "@angular/forms": "^15.2.0",
    "@angular/platform-browser": "^15.2.0",
    "@angular/platform-browser-dynamic": "^15.2.0",
    "@angular/router": "^15.2.0",
    "@porsche-design-system/components-angular": "^3.0.0-alpha.2",
    "@porsche-design-system/utilities": "^5.4.1",
    "@webcomponents/custom-elements": "^1.5.1",
    "ngx-build-plus": "^15.0.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.2.1",
    "@angular/cli": "~15.2.1",
    "@angular/compiler-cli": "^15.2.0",
    "@rollup/plugin-terser": "^0.4.0",
    "@types/jasmine": "~4.3.0",
    "@types/node": "^18.14.6",
    "concurrently": "^7.6.0",
    "i": "^0.3.7",
    "jasmine-core": "~4.5.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "npm": "^9.6.0",
    "rollup": "^3.18.0",
    "typescript": "~4.9.4",
    "webpack-bundle-analyzer": "^4.8.0"
  }
}
